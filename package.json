{
  "name": "visualmapper",
  "version": "1.2.0",
  "description": "A collection of visually accessible maps",
  "homepage": "http:/purvasingh96.github.io/accessible-maps",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css dist/",
    "start": "webpack-dev-server --progress --hot --open",
    "start-local": "webpack-dev-server --env local --progress --hot --open",
    "build": "npm run clean && tsc && cp package.json README.md ./dist && copyfiles -u 1 src/**/*.html src/**/*.css dist/",
    "predeploy": "gh-pages -d build",
    "deploy": "npm run build && npm run predeploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/purvasingh96/accessible-maps.git"
  },
  "dependencies": {
    "@emotion/react": "11.4.1",
    "@emotion/styled": "11.3.0",
    "@mapbox/geojson-rewind": "^0.5.2",
    "@mui/material": "5.0.2",
    "@storybook/addon-docs": "6.3.8",
    "@storybook/react": "6.3.8",
    "@turf/bbox": "^6.5.0",
    "@types/color": "^3.0.3",
    "babel-loader": "^8.0.4",
    "bootstrap": "^5.2.2",
    "color": "^4.2.3",
    "d3-array": "^3.1.1",
    "d3-scale": "^4.0.2",
    "date-fns": "^2.29.3",
    "mapbox-gl": "^2.0.0",
    "mapboxgl-legend": "^1.7.1",
    "node-sass": "^8.0.0",
    "papaparse": "^5.3.2",
    "react": "^18.2.0",
    "react-animated-cursor": "^2.5.2",
    "react-bootstrap": "^2.6.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.0.0",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.4.3",
    "react-scripts": "^3.0.1",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.6",
    "visualmapper": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.0.0",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^18.0.9",
    "copyfiles": "^2.4.1",
