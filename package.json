    "ts-loader": "^9.4.2",
    "typescript": "^4.0.0",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.7.0"
